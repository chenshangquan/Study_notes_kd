+u16 CCncSysCtrl::SetMoon904KCamCfgU8Cmd( const EmCamCfgType& emCamCfgType, const u8& wValue )
+{
+    CTpMsg *pcTpMsg = m_pSession->GetKdvMsgPtr();
+    pcTpMsg->SetUserData(m_pSession->GetInst());
+    pcTpMsg->SetBody(&emCamCfgType, sizeof(EmCamCfgType));
+    pcTpMsg->CatBody(&wValue,sizeof(u8));
+    pcTpMsg->SetEvent(ev_Cn_CamCfg_Cmd);
+    
+    u16 wRet = m_pSession->PostMsg(TYPE_TPMSG);
+    PrtMsg( ev_Cn_CamCfg_Cmd, emEventTypecnstoolSend, "EmCamCfgType:%d,Content:%d", emCamCfgType, wValue );
+    return wRet;
+}
+
+u16 CCncSysCtrl::SetMoon904KCamCfgU16Cmd( const EmCamCfgType& emCamCfgType, const u16& wValue )
+{
+    CTpMsg *pcTpMsg = m_pSession->GetKdvMsgPtr();
+    pcTpMsg->SetUserData(m_pSession->GetInst());
+    pcTpMsg->SetBody(&emCamCfgType, sizeof(EmCamCfgType));
+    pcTpMsg->CatBody(&wValue,sizeof(u16));
+    pcTpMsg->SetEvent(ev_Cn_CamCfg_Cmd);
+    
+    u16 wRet = m_pSession->PostMsg(TYPE_TPMSG);
+    PrtMsg( ev_Cn_CamCfg_Cmd, emEventTypecnstoolSend, "EmCamCfgType:%d,Content:%d", emCamCfgType, wValue );
+    return wRet;
+}
+
+void CCncSysCtrl::OnMoon904KCamCfgInd( const CMessage& cMsg )
+{
+	CTpMsg cTpMsg(&cMsg);
+    EmCamCfgType emCamCfgType = *(EmCamCfgType*)( cTpMsg.GetBody() );
+	BOOL bRet = *(BOOL*)( cTpMsg.GetBody() + sizeof(EmCamCfgType) );
+	PrtMsg( ev_Cn_CamCfg_Ind, emEventTypecnstoolRecv, "EmCamCfgType:%d,Result:%d", emCamCfgType, bRet );
+    PostEvent( UI_MOON904KCAMCFG_IND, (WPARAM)emCamCfgType, (LPARAM)bRet );
+}
