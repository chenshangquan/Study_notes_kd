<?xml version="1.0" encoding="utf-8" standalone="yes" ?>
<Window size="98,80" roundcorner="1,0">
<Font name="微软雅黑" size="12" bold="false" />
	<VerticalLayout name="SetMenuLayout" bkcolor="#FFFFFFFF" bordersize="1" bordercolor="#FF000000">
		<VerticalLayout height="24"/>
		<HorizontalLayout>
			<HorizontalLayout width="1"/>
			<VerticalLayout>
			<List name="SetMenuList" header="hidden" itemtextcolor="#FF4E4E4E" itembkcolor="#FFFFFFFF" itemhottextcolor="#FFFFFFFF" itemhotbkcolor="#FF1E94DA" >
				<ListContainerElement name="ModifyPasswrd" selected="false">
					<Label text="密码修改" textpadding="25,0,0,0" font="0" align="left" mouse="false" />
				</ListContainerElement>
				<ListContainerElement name="About" selected="false">
					<Label text="关于" textpadding="25,0,0,0" font="0" align="left" mouse="false"/>
				</ListContainerElement>
			</List >
			</VerticalLayout>
			<HorizontalLayout width="1"/>
		</HorizontalLayout>
		<VerticalLayout height="15"/>
		<Case name="caseSetMenuClose">	<!-- 关闭菜单 -->
			<ModifyPasswrd selected="false"/>
			<About selected="false"/>
		</Case>
	</VerticalLayout>
</Window>